{
  "name": "front_end",
  "version": "0.0.1",
  "description": "Website back end",
  "main": "index.js",
  "repository": "https://github.com/wallyfaye/website",
  "author": "Wally Faye",
  "license": "MIT",
  "scripts": {
    "docs": "esdoc",
    "start": "yarn run start:install --verbose && nodemon --exec babel-node ./src/index.js",
