    "babel-eslint": "*",
    "babel-plugin-dynamic-import-node": "*",
    "babel-jest": "*",
    "esdoc": "*",
    "esdoc-standard-plugin": "*",
    "eslint": "*",
    "eslint-loader": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jest": "*",
    "jest": "*",
    "koa": "*",
    "nodemon": "*"
  }
}
